<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapping.DeptInterface">
	<select id="getLink"  resultType="com.great.entity.Link">
		select * from tbl_link 
	</select>	
	
	<insert id="addLink" parameterType="com.great.entity.Link">
		insert  into  tbl_link(link_name,link_url) value(#{link_name},#{link_url})
	</insert>
	
	<delete id="delLink" parameterType="int">
		delete from  tbl_link where link_id=#{_parameter} 
	</delete>
</mapper>