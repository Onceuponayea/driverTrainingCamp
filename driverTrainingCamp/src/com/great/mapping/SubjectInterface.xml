<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.great.mapping.SubjectInterface">
	<!-- 根据学生id、科目获取学时详情 -->
	<select id="SubjectMapping" resultType="com.great.entity.SubjectTime">
		select * from
		tbl_subject_time where level_id=#{param1} and
		student_id=#{param2}
	</select>
	<!-- 添加学时详情 -->
	<insert id="addSubjectTime" parameterType="com.great.entity.SubjectTime">
		insert tbl_subject_time
		(level_id,student_id,subject_time_start,subject_time_end,subject_time_sum)values
		(#{level_id},#{student_id},#{subject_time_start},#{subject_time_end},#{subject_time_sum})
	</insert>
</mapper>