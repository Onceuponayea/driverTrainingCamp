<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapping.DeptInterface">
	<select id="getInfo" parameterType="string" resultType="com.great.entity.Crawler">
		select * from tbl_crawler 
		<where>
			<if test="_parameter!=null">   
				crawler_time>#{_parameter}
			</if>
		</where>
	</select>	
	
	<insert id="addInfo" parameterType="com.great.entity.Crawler">
		insert into  tbl_crawler(crawler_title,crawler_url,crawler_time) value(#{crawler_title},#{crawler_url},#{crawler_time})
	</insert>
 	
	<delete id="delInfo" parameterType="int">
		delete from  tbl_crawler where crawler_id=#{_parameter}
	</delete>
</mapper>